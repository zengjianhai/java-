package com.homework.three;

public class demo {

	public static void main(String[] args) {
		demo demo = new demo();
		demo.add(0, 0);
		
	}

	public int add(int count,int value){
		for (int i = 1; i <= 10; i++) {
			count = count+i;
			value =count+value;
			add(count,value);
		}
		return value;
	}
	public void ga(int count,int value){}
}
